<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bo.ucb.edu.ingsoft.dao.UserDao">
    <insert id="newUser" parameterType="bo.ucb.edu.ingsoft.model.User">
        INSERT INTO user
        VALUES (
            null,#{name},#{surname},#{username},
            #{email},'PasswordNeedsSecure',null,null,
            null,#{transaction.txId},#{transaction.txHost},#{transaction.txUserId},
            #{transaction.txDate}

        )
    </insert>

</mapper>
